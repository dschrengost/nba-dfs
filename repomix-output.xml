        import math

        def _clean_nans(obj):
            if isinstance(obj, float):
                return obj if math.isfinite(obj) else None
            if isinstance(obj, dict):
                return {k: _clean_nans(v) for k, v in obj.items()}
            if isinstance(obj, list):
                return [_clean_nans(v) for v in obj]
            return obj

