                        df = df.merge(pid_slim, on=join_keys, how="left", suffixes=("", "_pid"))
                        # If we already had a dk_id col, prefer non-null union; else adopt merged dk_id
                        if "dk_id" in df.columns and "dk_id_pid" in df.columns:
