    _grid_key = None
    try:
        _diag_all = st.session_state.get("id_diagnostics", {})
        _rid = None
        if isinstance(_diag_all, dict):
            _rid = _diag_all.get("solver_contract", {}).get("run_id") or _diag_all.get(
                "normalization", {}
            ).get("ownership", {}).get("run_id")
