{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["*"]
    },
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "include": ["app", "components", "lib", "styles"]
}
